<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    id="com.ionic.deploy"
    version="0.1.6">
    <name>IonicDeploy</name>
    <description>Ionic Deploy Plugin</description>
    <license>Apache 2.0</license>
    <keywords>Ionic,deploy</keywords>
    <repo>https://github.com/driftyco/ionic-plugins-deploy</repo>
    <issue>https://github.com/driftyco/ionic-plugins-deploy/issues</issue>

    <js-module src="www/ionicdeploy.js" name="IonicDeploy">
        <clobbers target="IonicDeploy" />
    </js-module>

    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="IonicDeploy">
                <param name="android-package" value="com.ionic.deploy.IonicDeploy"/>
                <param name="onload" value="true"/>
            </feature>
        </config-file>

        <source-file src="src/android/IonicDeploy.java" target-dir="src/com/ionic/deploy" />
    </platform>

    <!-- ios -->
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="IonicDeploy">
                <param name="ios-package" value="IonicDeploy" onload="true" />
            </feature>
        </config-file>

        <header-file src="src/ios/IonicDeploy.h" />
        <source-file src="src/ios/IonicDeploy.m" />
        <header-file src="src/ios/Download.h" />
